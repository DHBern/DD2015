	<text xml:lang="grc-c">
		<body>
			<div>
				<ab><milestone n="31r1" unit="folio"/>
					<lb n="1" facs="#zArch44r_31r1_01"/>εἰς τὸ χωρίον ἐγγράψαι σχᾶμα <lb n="2" facs="#zArch44r_31r1_02"
					/>ἐπίπεδον ἐξ ὁμοίων τομέων <lb n="3" facs="#zArch44r_31r1_03"/>συγκείμενον<pc>,</pc> ὥστε τὸ <choice>
						<abbr>εἰρημένο<am><g/></am></abbr>
						<expan>εἰρημένο<ex>ν</ex></expan>
					</choice>
					<lb n="4" facs="#zArch44r_31r1_04"/>χωρίον μείζονι μὲν τὸ τοῦ <w part="I">ἐγ</w>
					<lb n="5" facs="#zArch44r_31r1_05"/><w part="F">γραφέντος</w> σχάματος <w part="I">ἐλάσσο</w>
					<lb n="6" facs="#zArch44r_31r1_06"/><w part="F">νι</w> ἢ ἁλίκω ὑπερέχει τὸ αὐτὸ <w part="I">χω</w>
					<lb n="7" facs="#zArch44r_31r1_07"/><w part="F">ρίον</w> τοῦ Χ τομέως<pc>.</pc>
					<choice>
						<abbr>ἐγγεγράφθ<am><g/></am></abbr>
						<expan>ἐγγεγράφθ<ex>ω</ex></expan>
					</choice>
					<lb n="8" facs="#zArch44r_31r1_08"/>οὖν<pc>,</pc> καὶ ἔστω τῶν τομέων<pc>,</pc> ἐξ ὧν <lb n="9"
						facs="#zArch44r_31r1_09"/>σύγκειται τὸ ἐγγεγραμμένον <w part="I">σχᾶ</w>
					<lb n="10" facs="#zArch44r_31r1_10"/><w part="F">μα</w><pc>,</pc> μείζων μὲν ὁ ΘΒΓ<pc>,</pc> ἐλάσσων
						<lb n="11" facs="#zArch44r_31r1_11"/>δὲ ὁ ΘΕ<pc>·</pc> δῆλον οὖν<pc>,</pc>
					<choice>
						<abbr><am><g/></am></abbr>
						<expan><ex>ὅτι</ex></expan>
					</choice> τὸ <w part="I">ἐγγεγραμμέ</w>
					<lb n="12" facs="#zArch44r_31r1_12"/><w part="F">νον</w> σχᾶμα μεῖζόν <choice>
						<abbr><am><g/></am></abbr>
						<expan><ex>ἐστι</ex></expan>
					</choice> τοῦ Χ <w part="I">τομέ</w>
					<lb n="13" facs="#zArch44r_31r1_13"/><w part="F">ως</w><pc>.</pc> πάλιν οὖν ἐντί τινες <w part="I"
						>γραμ</w>
					<lb n="14" facs="#zArch44r_31r1_14"/><w part="F">μαὶ</w> τῶι ἴσωι ἀλλαλᾶν <w part="I">ὑπερέ</w>
					<lb n="15" facs="#zArch44r_31r1_15"/><w part="F">χουσαι</w> ἀπὸ τοῦ Θ ποτὶ τὰν <w part="I">ἕλι</w>
					<lb n="16" facs="#zArch44r_31r1_16"/><w part="F">κα</w> ποτιπίπτουσαι<pc>,</pc> ὧν ἐστιν <w part="I"
						>με</w>
					<lb n="17" facs="#zArch44r_31r1_17"/><w part="F">γίστα</w> μὲν ἁ ΘΑ<pc>,</pc> ἐλαχίστα δὲ ἁ <lb
						n="18" facs="#zArch44r_31r1_18"/>ΘΕ<pc>,</pc> ἐντὶ δὲ καὶ ἄλλαι <choice>
						<abbr>γραμμ<am><g/></am></abbr>
						<expan>γραμμ<ex>αὶ</ex></expan>
					</choice>
					<lb n="19" facs="#zArch44r_31r1_19"/>ἀπὸ <unclear>τοῦ</unclear> Θ ποτὶ τὰν τοῦ ΘΑΖ <w part="I"
						>το</w>
					<milestone n="32v1" unit="folio"/>
					<lb n="20" facs="#zArch44r_32v1_20"/><w part="F">μέως</w> περιφέρειαν <w part="I">ποτιπί</w>
					<lb n="21" facs="#zArch44r_32v1_21"/><w part="F">πτουσαι</w> χωρὶς τᾶς ΘΑ τῶι <choice>
						<abbr>μὲ<am><g/></am></abbr>
						<expan>μὲ<ex>ν</ex></expan>
					</choice>
					<lb n="22" facs="#zArch44r_32v1_22"/>πλήθει μιᾶ ἐλάσσονες τᾶν τῶι <lb n="23"
						facs="#zArch44r_32v1_23"/>ἴσωι ἀλλαλᾶν ὑπερεχουσᾶν<pc>,</pc>
					<lb n="24" facs="#zArch44r_32v1_24"/>τῶι δὲ μεγέθει ἀλλάλαις τε καὶ <lb n="25"
						facs="#zArch44r_32v1_25"/>τᾶ μεγίστα ἴσαι<pc>,</pc> καὶ <sic><w part="I">ἀναγεγρά</w></sic>
					<lb n="26" facs="#zArch44r_32v1_26"/><sic><w part="F">φεται</w></sic> ἀπὸ ἑκάστας ὁμοῖοι <w part="I"
						>το</w>
					<lb n="27" facs="#zArch44r_32v1_27"/><w part="F">μέες</w><pc>,</pc> ἀπὸ δὲ τᾶς μεγίστας τᾶν <lb
						n="28" facs="#zArch44r_32v1_28"/>τῶι ἴσωι ἀλλαλᾶν <choice>
						<abbr>ὑπερεχ<am><g/></am>σ<am><g/></am></abbr>
						<expan>ὑπερεχ<ex>ου</ex>σ<ex>ᾶν</ex></expan>
					</choice>
					<lb n="29" facs="#zArch44r_32v1_29"/><w><unclear>ο</unclear><supplied reason="lost"
						>ὐ</supplied>κ</w> ἀναγέγραπται<pc>·</pc> οἱ τομέες <lb n="30" facs="#zArch44r_32v1_30"
							/><w>ο<unclear>ὖ</unclear><supplied reason="lost">ν</supplied></w> οἱ ἀπὸ τᾶν ἰσᾶν ἀλλάλαις
						<lb n="31" facs="#zArch44r_32v1_31"/>τε καὶ τᾶ μεγίστα ποτὶ τοὺς <w part="I"><supplied
							reason="lost">το</supplied></w>
