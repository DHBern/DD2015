	<text xml:lang="grc-c">
		<body>
			<div>
				<ab><milestone n="102v1" unit="folio"/>
					<lb n="1" facs="#zArch39v_102v1_01"/>λαβόντα τὸ χωρίον τὸ <w part="I">περιεχό</w>
					<lb n="2" facs="#zArch39v_102v1_02"/><w part="F">μ<supplied reason="lost">ε</supplied>νον</w>
					<w>ὑπ<supplied reason="lost">ὸ</supplied></w> τᾶς ἕλικος τᾶς ἐν τᾶ <lb n="3"
						facs="#zArch39v_102v1_03"/><w>δευτέρ<supplied reason="lost">αι</supplied></w>
					<w>περι<supplied reason="lost">φ</supplied>ορᾶι</w>
					<w part="I">γεγραμμέ</w>
					<lb n="4" facs="#zArch39v_102v1_04"/><w part="F">νας</w> καὶ τᾶς <w><supplied reason="lost"
							>εὐ</supplied>θείας</w><pc>,</pc> ἅ ἐστι <w part="I">δευ</w>
					<lb n="5" facs="#zArch39v_102v1_05"/><w part="F">τέρα</w> τᾶν ἐν <w>τ<supplied reason="lost"
							>ᾶι</supplied></w> ἀρχᾶι τᾶς <w part="I">πε</w>
					<lb n="6" facs="#zArch39v_102v1_06"/><w part="F">ριφορᾶς</w><pc>,</pc>
					<w>δυνα<supplied reason="lost">τ</supplied>όν</w> ἐστιν περὶ <w part="I">αὐ</w>
					<lb n="7" facs="#zArch39v_102v1_07"/><w part="F">τὸ</w> σχᾶμα ἐπίπεδον <w part="I">περιγρά</w>
					<lb n="8" facs="#zArch39v_102v1_08"/><w part="F">ψαι</w> ἐξ ὁμοίων τομέων <w part="I">συγκεί</w>
					<lb n="9" facs="#zArch39v_102v1_09"/><w part="F">μενον</w>
					<w>κα<supplied reason="lost">ὶ</supplied></w> ἄλλο ἐγγράψαι<pc>,</pc> ὥστε <lb n="10"
						facs="#zArch39v_102v1_10"/>τὸ περιγραφὲν τοῦ <choice>
						<abbr>ἐγγραφέντ<am><g/></am></abbr>
						<expan>ἐγγραφέντ<ex>ος</ex></expan>
					</choice>
					<lb n="11" facs="#zArch39v_102v1_11"/>μεῖζον εἶμεν ἐλάσσονι παντὸς <lb n="12"
						facs="#zArch39v_102v1_12"/>τοῦ προτεθέντος χωρίου<pc>.</pc> ἔστω <w part="I">ἕ</w>
					<lb n="13" facs="#zArch39v_102v1_13"/><w part="F">λιξ</w><pc>,</pc> ἐφ’ ἇι ἁ ΑΒ ΓΔΕ<pc>,</pc> ἐν τᾶι
						<w part="I">δευτέ</w>
					<lb n="14" facs="#zArch39v_102v1_14"/><w part="F">ραι</w> περιφορᾶι γεγραμμένα<pc>,</pc>
					<lb n="15" facs="#zArch39v_102v1_15"/>καὶ ἔστω τὸ μὲν Θ σαμεῖον ἀρχὰ <lb n="16"
						facs="#zArch39v_102v1_16"/>τᾶς ἕλικος<pc>,</pc> ἁ δὲ ΑΘ ἀρχὰ τᾶς <w part="I">πε</w>
					<lb n="17" facs="#zArch39v_102v1_17"/><w part="F">ριφορᾶς</w><pc>,</pc> ἁ δὲ ΕΑ δευτέρα <sic><w
							part="I">εὐθεῖ</w></sic>
					<lb n="18" facs="#zArch39v_102v1_18"/><sic><w part="F">αι</w></sic> τᾶν ἐν τᾶι ἀρχᾶι <w>τᾶ<supplied
							reason="lost">ς</supplied></w>
					<choice>
						<abbr>περιφορ<am><g/></am></abbr>
						<expan>περιφορ<ex>ᾶς</ex></expan>
					</choice><pc>,</pc>
					<milestone n="98r1" unit="folio"/>
					<lb n="19" facs="#zArch39v_98r1_19"/>ὁ δὲ ΑΖΗ κύκλος ἔστω δεύτερος καὶ <lb n="20"
						facs="#zArch39v_98r1_20"/><supplied reason="lost">αἱ</supplied> ΑΓ <unclear>Η</unclear>ΖΙ
							<w><supplied reason="lost">δ</supplied>ιάμ<unclear>ε</unclear>τρ<supplied reason="lost"
							>ο</supplied>ι</w> αὐτοῦ πρὸς <w part="I">ὀρ</w>
					<lb n="21" facs="#zArch39v_98r1_21"/><w part="F">θὰς</w>
					<w>ἀλλ<supplied reason="lost">ά</supplied>λαις</w><pc>.</pc> πάλιν οὖν δίχα <lb n="22"
						facs="#zArch39v_98r1_22"/>τεμνομένας τᾶς ὀρθᾶς <choice>
						<abbr>γωνί<am><g/></am></abbr>
						<expan>γωνί<ex>ας</ex></expan>
					</choice>
					<lb n="23" facs="#zArch39v_98r1_23"/>καὶ τοῦ τομέως τοῦ τὴν ὀρθὴν <lb n="24"
						facs="#zArch39v_98r1_24"/>γωνίαν περιέχοντος ἐσσεῖται <lb n="25" facs="#zArch39v_98r1_25"/>τὸ
					καταλειπόμενον ἔλασσον <lb n="26" facs="#zArch39v_98r1_26"/>τοῦ προτεθέντος<pc>·</pc> καὶ ἔστω <w
						part="I">γεγε</w>
					<lb n="27" facs="#zArch39v_98r1_27"/><w part="F">νημένος</w> ὁ ΘΚ<supplied reason="lost"
						>Α</supplied> τομεὺς <choice>
						<abbr>ἐλάσσω<am><g/></am></abbr>
						<expan>ἐλάσσω<ex>ν</ex></expan>
					</choice>
					<lb n="28" facs="#zArch39v_98r1_28"/>τοῦ προτεθέντος <w>χ<unclear>ω</unclear>ρίου</w><pc>.</pc>
					<w part="I">διαι</w>
					<lb n="29" facs="#zArch39v_98r1_29"/><w part="F">ρεθεισᾶν</w> δὴ τᾶν ὀρθᾶν <w part="I">γωνι</w>
