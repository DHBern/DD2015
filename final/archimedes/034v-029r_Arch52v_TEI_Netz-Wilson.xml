						<expan>π<ex>αρὰ</ex></expan>
					</choice>
					<lb n="8" facs="#zArch52v_34v1_08"/>τὴν βάσιν<pc>,</pc>
					<choice>
						<abbr><am><g/></am></abbr>
						<expan><ex>ἔστιν</ex></expan>
					</choice><pc>,</pc> ὧι ὁ κύλινδρος <choice>
						<abbr><am><g/></am></abbr>
						<expan><ex>πρὸς</ex></expan>
					</choice>
					<choice>
						<abbr>τὸ<am><g/></am></abbr>
						<expan>τὸ<ex>ν</ex></expan>
					</choice>
					<lb n="9" facs="#zArch52v_34v1_09"/>κύλινδρον<pc>,</pc> ὁ ἄξων πρὸς τὸν ἄξονα<pc>.</pc>
